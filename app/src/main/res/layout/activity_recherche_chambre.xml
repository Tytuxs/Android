<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RechercheChambre">

    <Button
            android:text="Rechercher"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button" tools:layout_editor_absoluteY="667dp"
            tools:layout_editor_absoluteX="139dp" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Date souhaitée"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2" tools:layout_editor_absoluteY="144dp"
            tools:layout_editor_absoluteX="16dp" tools:ignore="MissingConstraints"/>
    <Spinner
            android:layout_width="260dp"
            android:layout_height="39dp" tools:layout_editor_absoluteX="13dp" android:id="@+id/spinnerNbPersonne"
            tools:layout_editor_absoluteY="251dp" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Réservation Chambre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3" tools:layout_editor_absoluteY="16dp"
            tools:layout_editor_absoluteX="138dp" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="Date"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="AAAA-MM-JJ"
            android:ems="10"
            android:id="@+id/editTextDate" tools:layout_editor_absoluteY="166dp"
            tools:layout_editor_absoluteX="14dp" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Nom Personne Référence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4" tools:layout_editor_absoluteY="56dp"
            tools:layout_editor_absoluteX="17dp" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Nombre Personnes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5" tools:layout_editor_absoluteY="232dp"
            tools:layout_editor_absoluteX="16dp" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editTextName" tools:layout_editor_absoluteY="78dp"
            tools:layout_editor_absoluteX="16dp" tools:ignore="MissingConstraints"/>
</androidx.constraintlayout.widget.ConstraintLayout>